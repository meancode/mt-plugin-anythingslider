<div id="page-<mt:PageID />" class="page-asset asset">
<div class="asset-header">
<h1 class="asset-name"><mt:PageTitle /></h1>
</div>
<div class="asset-content">
<mt:IfNonEmpty tag="PageBody">
<div class="asset-body">
<mt:PageBody />
</div>
</mt:IfNonEmpty>
<mt:IfNonEmpty tag="PageMore" convert_breaks="0">
<div id="more" class="asset-more">
<mt:PageMore />
</div>
</mt:IfNonEmpty>
</div>
</div>